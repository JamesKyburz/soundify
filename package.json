{
  "name": "soundify",
  "description": "stream tracks to devices from multiple sources",
  "version": "0.0.10",
  "repository": {
    "type": "git",
    "url": "git://github.com/jameskyburz/soundify.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "npm install && node ./node_modules/supervisor/lib/cli-wrapper.js -w . -e \"js|css|html|json\" index.js"
  },
  "author": {
    "name": "James Kyburz",
    "email": "james.kyburz@gmail.com"
  },
  "keywords": [
    "music",
    "player",
    "streaming"
  ],
  "license": "MIT",
  "dependencies": {
    "JSONStream": "0.8.4",
    "cookie-cutter": "0.1.1",
    "hyperdirect": "0.0.0",
    "hyperglue": "1.3.3",
    "hyperquest": "0.3.0",
    "lame": "1.2.3",
    "speaker": "0.2.6",
    "spotify-web": "git+ssh://git@github.com/fuzeman/node-spotify-web.git#feature/ping-pong-fix",
    "stack": "0.1.0",
    "supervisor": "0.9.1",
    "through": "2.3.8",
    "tiny-route": "2.1.1",
    "youtube-audio-stream": "0.0.7"
  }
}
